{
  "manifest_version": 2,
  "name": "Mindload",
  "description": "Capture your brain",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "Mindload"
  },
  "options_ui": {
    "page": "options.html"
  }
}
